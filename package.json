{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "This repo contains a basic Node and Express app to get you started in constructing an API. To get started, clone this repo and run `yarn` in your terminal at the project root.",
  "main": "server.ts",
  "scripts": {
    "start": "npx ts-node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "set ENV=test && npx tsc && db-migrate -c 4 up --config database.json -e test && jasmine --config=spec/support/jasmine.json && db-migrate -c 4 down --config database.json -e test",
    "tsc": "tsc",
    "lint": "npx eslint src/**/*.ts",
    "prettier": "prettier --config .prettierrc \"src\\**\\*.ts\" --write",
    "db-up": "db-migrate up",
    "db-down": "db-migrate down"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "migrate": "^2.1.0",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jasmine": "^5.1.5",
    "@types/node": "^22.9.2",
    "@types/pg": "^7.14.7",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "jasmine": "^5.5.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "tsc-watch": "^4.2.9",
    "typescript-eslint": "^8.15.0"
  }
}
